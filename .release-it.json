{
  "git": {
    "commitMessage": "chore(lib): release version ${version} [skip ci]",
    "requireCleanWorkingDir": false
  },
  "github": {
    "release": true,
    "releaseName": "${version}"
  },
  "npm": {
    "ignoreVersion": true
  },
  "plugins": {
    "@csmith/release-it-calver-plugin" : {
      "format": "YYYY.MM.DD"
    }
  }
}
