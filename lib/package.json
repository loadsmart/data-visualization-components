  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "release-it": "^14.6.2"
  },
  "files": [
    "dist"
  ]
